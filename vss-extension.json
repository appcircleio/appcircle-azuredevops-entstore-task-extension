{
  "manifestVersion": 1,
  "id": "enterprise-app-store",
  "name": "Enterprise App Store",
  "version": "0.0.29",
  "publisher": "Appcircle",
  "public": true,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "tags": [
    "app store",
    "google play",
    "Beta Distribution",
    "Mobile",
    "iOS",
    "Android",
    "IPA",
    "APK/AAB"
  ],
  "description": "Appcircle Enterprise Mobile App Store is your own mobile app store for providing access to in-house apps with a customizable mobile storefront.",
  "categories": ["Azure Pipelines"],
  "icons": {
    "default": "images/extension-icon.png"
  },
  "files": [
    {
      "path": "task"
    },
    {
      "path": "images",
      "addressable": true
    }
  ],
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "contributions": [
    {
      "id": "custom-build-release-task",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "task"
      }
    }
  ]
}
